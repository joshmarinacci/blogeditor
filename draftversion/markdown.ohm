Markdown {
    doc = para+
    para = (~"\n\n" body)* "\n\n"
    body = link | image | strong | plainrun
    strong = "*" plain+ "*"
    plainrun = plain+
    plain = (~("[" | "*" | "\n\n") any)
    image = "!" "[" desc "]" "(" url ")"
    link  = "[" desc "]" "(" url ")"
    desc = (~( "]" ) any)*
    url = (~( ")") any)*
}
